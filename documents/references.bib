@article{Time2020,
  title = {Time {{Course}} of {{Lung Changes}} at {{Chest CT}} during {{Recovery}} from {{Coronavirus Disease}} 2019 ({{COVID}}-19)},
  author = {Pan, Feng and Ye, Tianhe and Sun, Peng and Gui, Shan and Liang, Bo and Li, Lingli and Zheng, Dandan and Wang, Jiazheng and Hesketh, Richard L. and Yang, Lian and Zheng, Chuansheng},
  year = {2020},
