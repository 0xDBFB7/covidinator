@article{condon_collapse_nodate,
	title = {Collapse of the 300 foot telescope},
	abstract = {The 300 foot telescope was the ﬁrst large telescope “readily available to all comers” and taught the NRAO how to run a national observatory. Although it was conceived as a “quick and dirty” stopgap with a ﬁve-year lifetime, the 300 foot telescope was upgraded many times and remained scientiﬁcally productive for 26 years. Its unexpected collapse in 1988 paved the way for its vastly improved successor, the GBT, and may have saved radio astronomy in Green Bank. Friends of the 300 foot fondly remember it as a symbol of radio astronomy in its bold, creative, simple, and sometimes foolish younger days.},
	language = {en},
	author = {Condon, James J},
	pages = {10},
	annote = {A panel of engineers investigated the collapse and issued their report in March 1989. Chairman Robert M. Matyas, a management consultant in Ithaca, N.Y., told ENR [Engineering News-Record] that a gusset plate connecting truss members “where the load [of the dish] was transmitted to the bearing . . . had been cracking for years and finally ran out of cross section.” Noting that the structure lasted “two or three times” its design life, Matyas said there was “no way the collapse could have been anticipated. Progressive fatigue cracking is microscopic” and detection of a gusset-plate crack among the highly interdependent structural members would have required disassembly. However, it turns out that the collapse could have been anticipated without disassembly because astronomical observations are exquisitely sensitive to tele- ZAPPED! .. . by Hostile Space Aliens! 329 scope geometry. When I later found time to analyze the 1988 data from the GB6 survey, I discovered that the telescope had already been failing at the beginning of my observing run a month prior to the collapse. In 1986 and 1987 the pointing hysteresis (the mean difference between positions measured by north-going and south-going scans) had been about 10 arcsec; in 1988 the hysteresis had grown to 30–50 arcsec. Even more ominously, during the final week before the collapse the north-south beamwidth increased from 3 arcmin to 4–5 arcmin. Had I been able to keep up with the data while observing, I would have realized that the dish was opening up by a significant fraction of the 6 cm wavelength and stopped the telescope. The 300 foot would not have collapsed, there would have been no disaster to justify emergency funding for the GBT, and the Green Bank site might have been closed. In this case, ignorance really was bliss: Green Bank may have been saved by the slow computers of the day and by inefficient single-dish analysis software.},
	file = {Condon - ZAPPED! . . . by Hostile Space Aliens!.pdf:/home/arthurdent/Zotero/storage/RKZSX7J8/Condon - ZAPPED! . . . by Hostile Space Aliens!.pdf:application/pdf}
}

