	pages = {10},
	file = {Altaher_Aphale_2018_High-Precision Control of a Piezo-Driven Nanopositioner Using Fuzzy Logic.pdf:/home/arthurdent/Zotero/storage/UZE3XB9H/Altaher_Aphale_2018_High-Precision Control of a Piezo-Driven Nanopositioner Using Fuzzy Logic.pdf:application/pdf;Snapshot:/home/arthurdent/Zotero/storage/BEEEB5CD/10.html:text/html}
