@article{Communication2013,
  ids = {myersCommunicationOriginContributions2013},
  title = {Communication: {{Origin}} of the Contributions to {{DNA}} Structure in Phages},
  shorttitle = {Communication},
  author = {Myers, Christopher G. and Pettitt, B. Montgomery},
  year = {2013},
  month = feb,
  volume = {138},
  issn = {0021-9606},
  doi = {10.1063/1.4791708},
