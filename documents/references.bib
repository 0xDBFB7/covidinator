  shorttitle = {Flexible Bronchoscopy-Guided Microwave Ablation in Peripheral Porcine Lung},
  author = {Yuan, Hai-Bin and Wang, Xiang-Yu and Sun, Jia-Yuan and Xie, Fang-Fang and Zheng, Xiao-Xuan and Tao, Guang-Yu and Pan, Lei and Hogarth, Douglas Kyle},
  date = {2019-12},
