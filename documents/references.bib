  author = {Serwer, Philip and Moreno, Elena T. and Hayes, Shirley J. and Berger, Philip and Langham, Marie and Toler, Robert W.},
  date = {1984},
  journaltitle = {ELECTROPHORESIS},
  volume = {5},
