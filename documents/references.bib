@article{Safe2016,
  title = {Safe and Cost-Effective Rapid-Prototyping of Multilayer {{PMMA}} Microfluidic Devices},
  author = {Liga, Antonio and Morton, Jonathan A. S. and Kersaudy-Kerhoas, Maïwenn},
  date = {2016-11-25},
  journaltitle = {Microfluidics and Nanofluidics},
  shortjournal = {Microfluid Nanofluid},
  volume = {20},
  pages = {164},
  issn = {1613-4990},
