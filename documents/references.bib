@article{Thermoacoustic2017,
  title = {Thermoacoustic {{Imaging}} and {{Therapy Guidance}} Based on {{Ultra}}-Short {{Pulsed Microwave Pumped Thermoelastic Effect Induced}} with {{Superparamagnetic Iron Oxide Nanoparticles}}},
  author = {Wen, Liewei and Yang, Sihua and Zhong, Junping and Zhou, Quan and Xing, Da},
