}

@article{Rapid2020,
  title = {Rapid {{Detection}} of {{COVID}}-19 {{Causative Virus}} ({{SARS}}-{{CoV}}-2) in {{Human Nasopharyngeal Swab Specimens Using Field}}-{{Effect Transistor}}-{{Based Biosensor}}},
  author = {Seo, Giwan and Lee, Geonhee and Kim, Mi Jeong and Baek, Seung-Hwa and Choi, Minsuk and Ku, Keun Bon and Lee, Chang-Seop and Jun, Sangmi and Park, Daeui and Kim, Hong Gi and Kim, Seong-Jun and Lee, Jeong-O and Kim, Bum Tae and Park, Edmond Changkyun and Kim, Seung Il},
  year = {2020},
  month = apr,
  volume = {14},
  pages = {5135--5142},
  publisher = {{American Chemical Society}},
  issn = {1936-0851},
  doi = {10.1021/acsnano.0c02823},
  url = {https://doi.org/10.1021/acsnano.0c02823},
  urldate = {2020-08-17},
  abstract = {Coronavirus disease 2019 (COVID-19) is a newly emerging human infectious disease caused by severe acute respiratory syndrome coronavirus 2 (SARS-CoV-2, previously called 2019-nCoV). Based on the rapid increase in the rate of human infection, the World Health Organization (WHO) has classified the COVID-19 outbreak as a pandemic. Because no specific drugs or vaccines for COVID-19 are yet available, early diagnosis and management are crucial for containing the outbreak. Here, we report a field-effect transistor (FET)-based biosensing device for detecting SARS-CoV-2 in clinical samples. The sensor was produced by coating graphene sheets of the FET with a specific antibody against SARS-CoV-2 spike protein. The performance of the sensor was determined using antigen protein, cultured virus, and nasopharyngeal swab specimens from COVID-19 patients. Our FET device could detect the SARS-CoV-2 spike protein at concentrations of 1 fg/mL in phosphate-buffered saline and 100 fg/mL clinical transport medium. In addition, the FET sensor successfully detected SARS-CoV-2 in culture medium (limit of detection [LOD]: 1.6 \texttimes{} 101 pfu/mL) and clinical samples (LOD: 2.42 \texttimes{} 102 copies/mL). Thus, we have successfully fabricated a promising FET biosensor for SARS-CoV-2; our device is a highly sensitive immunological diagnostic method for COVID-19 that requires no sample pretreatment or labeling.},
  file = {/home/arthurdent/Zotero/storage/3QN5AN4Y/Seo et al_2020_Rapid Detection of COVID-19 Causative Virus (SARS-CoV-2) in Human.pdf;/home/arthurdent/Zotero/storage/7KX2V8IV/acsnano.html},
  journal = {ACS Nano},
  number = {4},
  itemID = 3961
}

