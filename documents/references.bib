  author = {Chemeris, Nikolai K. and Gapeyev, Andrew B. and Sirota, Nikolai P. and Gudkova, Olga Yu. and Kornienko, Natalia V. and Tankanag, Arina V. and Konovalov, Igor V. and Buzoverya, Marina E. and Suvorov, Valeriy G. and Logunov, Vyacheslav A.},
