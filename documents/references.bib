@article{winter_coarse_2009,
	title = {Coarse {Grained} {Simulations} of a {Small} {Peptide}: {Effects} of {Finite} {Damping} and {Hydrodynamic} {Interactions}},
	volume = {131},
	issn = {00219606},
	shorttitle = {Coarse {Grained} {Simulations} of a {Small} {Peptide}},
	url = {http://arxiv.org/abs/0908.1685},
	doi = {10.1063/1.3216573},
	abstract = {In the coarse grained Brownian Dynamics simulation method the many solvent molecules are replaced by random thermal kicks and an effective friction acting on the particles of interest. For Brownian Dynamics the friction has to be so strong that the particles' velocities are damped much faster than the duration of an integration timestep. Here we show that this conceptual limit can be dropped with an analytic integration of the equations of damped motion. In the resulting Langevin integration scheme our recently proposed approximate form of the hydrodynamic interactions between the particles can be incorparated conveniently, leading to a fast multi-particle propagation scheme, which captures more of the short-time and short-range solvent effects than standard BD. Comparing the dynamics of a bead-spring model of a short peptide, we recommend to run simulations of small biological molecules with the Langevin type finite damping and to include the hydrodynamic interactions.},
	number = {10},
	urldate = {2020-04-22},
	journal = {The Journal of Chemical Physics},
	author = {Winter, Uwe and Geyer, Tihamer},
	year = {2009},
	note = {arXiv: 0908.1685},
	pages = {104102},
	annote = {Comment: 7 pages, 6 figures, submitted to J Chem Phys},
	file = {arXiv.org Snapshot:/home/arthurdent/Zotero/storage/R6S8KZBY/0908.html:text/html;Winter_Geyer_2009_Coarse Grained Simulations of a Small Peptide.pdf:/home/arthurdent/Zotero/storage/EVYWQTQP/Winter_Geyer_2009_Coarse Grained Simulations of a Small Peptide.pdf:application/pdf}
