  shorttitle = {Half a {{Century}} of {{Research}} on {{Membrane}}-{{Containing Bacteriophages}}},
  author = {M{\"a}ntynen, Sari and Sundberg, Lotta-Riina and Oksanen, Hanna M. and Poranen, Minna M.},
  year = {2019},
