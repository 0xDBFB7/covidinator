  shorttitle = {An Effective Approach to Reduce the Penetration Potential of {{Sars}}-{{Cov}}-2 and Other Viruses by Spike Protein},
  author = {Rakshit, Kausik and Chatterjee, Sudip and Bandyopadhyay, Durjoy and Sarkar, Somsekhar},
