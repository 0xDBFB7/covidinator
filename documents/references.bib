	file = {Hardell_2017_World Health Organization, radiofrequency radiation and health - a hard nut to.pdf:/home/arthurdent/Zotero/storage/SFNPWQGV/Hardell_2017_World Health Organization, radiofrequency radiation and health - a hard nut to.pdf:application/pdf}
}

